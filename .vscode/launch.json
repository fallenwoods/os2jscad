{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

    {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome",
        "url": "http://localhost:8080",
        "webRoot": "${workspaceFolder}"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch Node",
        "program": "${file}"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch OpenScad",
        "args": ["examples/pins/pin_joints.scad"],
        "program": "${workspaceFolder}/OpenScadTest.js"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch Test",
        "args": ["-e","js","examples/test.scad"],
        "program": "${workspaceFolder}/os2jscadMain.js"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Test w Stubs",
        "args": ["-e","js","-s","examples/test.scad"],
        "program": "${workspaceFolder}/os2jscadMain.js"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch Includes",
        "args": ["-e","js","-i","includeTest4/main*.scad"],
        "program": "${workspaceFolder}/os2jscadMain.js"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch examples",
        "args": ["examples/*.scad"],
        "program": "${workspaceFolder}/os2jscadMain.js"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch jscadTest.js",
        "program": "${workspaceFolder}/jscadTest.js"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch output.js",
        "program": "${workspaceFolder}/output.js"
    }
    ]
}